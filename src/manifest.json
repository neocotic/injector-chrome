{
    "content_scripts": [
        {
            "all_frames": true
          , "js": [
                "lib/vendor/jquery.js"
              , "lib/vendor/underscore.js"
              , "lib/vendor/backbone.js"
              , "lib/vendor/backbone.chromestorage.js"
              , "lib/vendor/coffee-script.js"
              , "lib/models.js"
              , "lib/runner.js"
            ]
          , "matches": ["*://*/*"]
          , "run_at": "document_start"
        }
      , {
            "js": ["lib/install.js"]
          , "matches": ["http://*.neocotic.com/script-runner/*"]
          , "run_at": "document_end"
        }
    ]
  , "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://*.uservoice.com; object-src 'self'"
  , "default_locale": "en"
  , "description": "__MSG_description__"
  , "homepage_url": "http://neocotic.com/script-runner"
  , "manifest_version": 2
  , "minimum_chrome_version": "22"
  , "name": "__MSG_name__"
  , "options_page": "pages/options.html"
  , "permissions": [
        "storage"
      , "tabs"
      , "unlimitedStorage"
      , "<all_urls>"
    ]
  , "version": "1.0.0"
}
